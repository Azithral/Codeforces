using namespace std;

#define fi           first
#define se           second
#define pb           push_back
#define INF          1000000000
#define ll           long long
#define vi           vector<int>
#define vit          vector<int>::iterator
#define pii          pair<int,int>
#define FOR(a,b,c)   for(int a=b;a<c;a++)
#define REP(i,n)     FOR(i,0,n)

int main()
{
    string a;
    cin>>a;
    string b;
    cin>>b;

    int c[a.length()];
    REP(i,a.length())
    {
        if(a[i] != b[i])
        {
            c[i] = 1;
        }
        if(a[i] == b[i])
        {
            c[i] = 0;
        }
        cout<<c[i];
    }




}
